@page
@model WebApplication1.Pages.LoginModel

<h1>Login Page</h1>
<form method="post">

    <div class="form-group mb-3">
        <label asp-for="@Model.Username" class="form-label">Username</label>
        <input asp-for="@Model.Username" type="text" class="form-control"/>
    </div>

    <div class="form-group mb-3">
        <label asp-for="@Model.Password" class="form-label">Password</label>
        <input asp-for="@Model.Password" type="password" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-primary" style="width:auto">Submit</button>
</form>